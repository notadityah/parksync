{
  "name": "parksync",
  "version": "1.0.0",
  "description": "Real-time parking availability system for Melbourne CBD",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python -m uvicorn app:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && python -m pytest",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && black . && flake8",
    "lint:frontend": "cd frontend && npm run lint",
    "setup": "./scripts/setup.sh",
    "db:init": "cd backend && python db_manager.py init",
    "db:reset": "cd backend && python db_manager.py reset",
    "db:test": "cd backend && python db_manager.py test"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "parking",
    "melbourne",
    "fastapi",
    "react",
    "vite",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/notadityah/parksync-frontend.git"
  },
  "author": "Your Name"
}
